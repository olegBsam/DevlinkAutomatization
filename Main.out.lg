Процедура Gidrolock_Monitoring
Начало
  Константы
    Начало
    Конец
  Переменные
    Начало
      Лог openCrane
      Лог closeCrane
      Лог isOpenCmdCur
      Цел16 craneState
    Конец
  Для iOpcIsConnectionGidrolockCrane[7], iMdbIsConnectionGidrolockCrane[21], iMdbGidrolockCraneWireOpen[13], iMdbGidrolockCraneWireClose[14], iOpcGidrolockCraneState[9], iMdbCraneId[12]
  {
    рв[iOpcIsConnectionGidrolockCrane] = лв(пл[iMdbIsConnectionGidrolockCrane])
    openCrane = пл[iMdbGidrolockCraneWireOpen]
    closeCrane = пл[iMdbGidrolockCraneWireClose]
    craneState = Decoder_GetGidrolockCraneState(openCrane, closeCrane)
    isOpenCmdCur = пл[iMdbCraneId]
    Если (craneState = 2)
    {
      Если isOpenCmdCur
      {
        рв[iOpcGidrolockCraneState] = 5.00000
      }
      Иначе
      {
        Если (!isOpenCmdCur)
        {
          рв[iOpcGidrolockCraneState] = 4.00000
        }
        
      }
      
    }
    Иначе
    {
      Если (craneState # 3)
      {
        рв[iOpcGidrolockCraneState] = цв(craneState)
      }
      
    }
    
  }
  
Конец

